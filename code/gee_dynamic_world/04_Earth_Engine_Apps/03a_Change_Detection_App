// Panels are the main container widgets
var mainPanel = ui.Panel({
  style: {width: '300px'}
});


var title = ui.Label({
  value: 'Dynamic World Change Detection',
  style: {'fontSize': '24px'}
});
// You can add widgets to the panel
mainPanel.add(title)

Map.setCenter(37.794, 0.341, 7)
ui.root.add(mainPanel); 
